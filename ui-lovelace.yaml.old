resources:
  - url: /local/custom-lovelace/card-modder.js?v=2
    type: js
  - url: /local/custom-lovelace/card-tools.js?v=0.3
    type: js
  - url: /local/custom-lovelace/compact-custom-header.js?v=0.0.3
    type: module
  - url: /customcards/github/custom-cards/tracker-card.js?track=true
    type: module
  - url: /local/custom-lovelace/slider-entity-row.js
    type: js
  - url: /local/custom-lovelace/circle-sensor-card.js
    type: module
  - url: /local/custom-lovelace/tiles-card.js
    type: js

background: var(--background-image)

title: Dashboard
views:
##Main Tab#############################################################################################
  # - !include lovelace/01_home_view.yaml
  - title: Main
    id: 0
    icon: mdi:home-heart
    cards:
##Compact Header Config
      - type: custom:compact-custom-header
        main_config: true
        swipe: true
        background: '#636B75'
        menu: show
        options: show
        voice: hide
        notifications: clock
        clock_format: '12'
        clock_date: true
        disable: false
        exceptions:
          - conditions:
              user: mj
            config:
              hide_tabs: 7,8
          - conditions:
              user_agent: Mobile
              #media_query: '(max-width: 600px)'
            config:
              hide_tabs: 0,1,2,3,4,5,6,7,8
          - conditions:
              user_agent: Windows NT
            config:
              hide_tabs: 9

##Home -
      # - type: horizontal-stack
      #   cards:
      - type: entities
        show_header_toggle: false
        title: Status
        entities:
          - sensor.sen_home_status
          # - binary_sensor.alarm
          # - binary_sensor.windows
          # - binary_sensor.garage
          - type: divider
          - person.josh
          - person.mj
          # - type: divider
          # - binary_sensor.trash
##Parlor light
      - type: custom:tiles-card
        card_settings:
          background: var(--paper-card-background-color)
          columns: 4
          gap: 24px
          title: Parlor
        global_settings:
          background:
            value: none
          # icon:
          #   color: var('--paper-item-icon-color')
          #   size: 2.3em
        entities:
          - entity: light.living_room_lamps #(Entity specific settings. Overwrite the global settings)
            icon:
              value: mdi:lamp
              color:  #(optional)
                value_on: var(--accent-color) #(optional)
                value_off: var(--text-color) #(optional)
              size: 2.3em #(optional)
            label_sec: Lamps
          - entity: light.corner_lamp #(Entity specific settings. Overwrite the global settings)
            icon:
              value: mdi:lightbulb
              color:  #(optional)
                value_on: var(--accent-color) #(optional)
                value_off: var(--text-color) #(optional)
              size: 2.3em #(optional)
            label_sec: Corner
          - entity: light.table_lamp #(Entity specific settings. Overwrite the global settings)
            icon:
              value: mdi:lightbulb
              color:  #(optional)
                value_on: var(--accent-color) #(optional)
                value_off: var(--text-color) #(optional)
              size: 2.3em #(optional)
            label_sec: Table
##Kitchen/Dining light
      - type: custom:tiles-card
        card_settings:
          background: var(--paper-card-background-color)
          columns: 4
          gap: 24px
          title: Kitchen
        global_settings:
          background:
            value: none
          # icon:
          #   color: var('--paper-item-icon-color')
          #   size: 2.3em
        entities:
          - entity: light.dining_room #(Entity specific settings. Overwrite the global settings)
            icon:
              value: mdi:ceiling-light
              color:  #(optional)
                value_on: var(--accent-color) #(optional)
                value_off: var(--text-color) #(optional)
              size: 2.3em #(optional)
            label_sec: Dining
          - entity: light.kitchen #(Entity specific settings. Overwrite the global settings)
            icon:
              value: mdi:ceiling-light
              color:  #(optional)
                value_on: var(--accent-color) #(optional)
                value_off: var(--text-color) #(optional)
              size: 2.3em #(optional)
            label_sec: Kitchen



##Security Tab#############################################################################################
  - title: Security
    id: 1
    icon: mdi:shield-home
    cards:
##Compact Header Config
      - type: custom:compact-custom-header

      - type: picture-entity
        entity: camera.inside_backdoor
        name: Inside Backdoor Still

      - type: picture-entity
        entity: camera.test
        name: test



      # - type: picture-entity
        # entity: camera.inside_backdoor
        # name: Inside Backdoor Live Stream
        # camera_view: live

      # - type: picture-glance
        # entities: []
        # camera_image: camera.inside_backdoor


##Parlor Tab#############################################################################################
  - title: Parlor
    id: 2
    icon: mdi:piano
    cards:
##Compact Header Config
      - type: custom:compact-custom-header

      - type: light
        entity: light.living_room_lamps
      - type: horizontal-stack
        cards:
          - type: light
            entity: light.corner_lamp
          - type: light
            entity: light.table_lamp
      - type: entities
        title: Parlor
        show_state: false
        entities:
          - entity: group.living_room
            icon: mdi:lamp
            name: All
            tap_action: toggle
          - entity: group.living_room_lamps
            icon: mdi:lamp
            name: Lamps
            tap_action: toggle
          - entity: light.corner_lamp
            tap_action: toggle
          - entity: light.table_lamp
            tap_action: toggle
          - entity: light.front_door_light
            tap_action: toggle
          - type: custom:slider-entity-row
            entity: light.hall
          - entity: light.paper_lamp
            icon: mdi:floor-lamp
            tap_action: toggle

##Kitchen Tab#############################################################################################
  - title: Kitchen
    id: 3
    icon: mdi:silverware
    cards:
##Compact Header Config
      - type: custom:compact-custom-header

      - type: horizontal-stack
        cards:
          - type: light
            entity: light.dining_one
          - type: light
            entity: light.dining_two
          - type: light
            entity: light.dining_room


##Family Room Tab#############################################################################################
  - title: Family Room
    id: 4
    icon: mdi:sofa
    cards:
##Compact Header Config
      - type: custom:compact-custom-header

      - type: horizontal-stack
        cards:
          - type: light
            entity: light.eugene
          - type: light
            entity: light.rapunzel
      - type: entity-button
        entity: switch.bed_fan

##Offcie Tab#############################################################################################
  - title: Office
    id: 5
    icon: mdi:desktop-tower-monitor
    cards:
##Compact Header Config
      - type: custom:compact-custom-header

      - type: picture-glance
        title: Office
        show_state: false
        camera_image: camera.octoprint
        entities:
          - entity: switch.desk_light
            icon: mdi:desk-lamp
            tap_action: toggle
          - entity: switch.office_light
            tap_action: toggle
          - entity: switch.office_fan
            icon: mdi:fan
            tap_action: toggle
          - entity: sensor.test_temp
          - entity: sensor.motion_test
            icon: mdi:run
      - type: sensor
        entity: sensor.test_temp
        name: Office Temp
        hours_to_show: 72

##Master Bedroom Tab#############################################################################################
  - title: Master Bedroom
    id: 6
    icon: mdi:hotel
    cards:
##Compact Header Config
      - type: custom:compact-custom-header

      - type: horizontal-stack
        cards:
          - type: light
            entity: light.eugene
          - type: light
            entity: light.rapunzel
      - type: entity-button
        entity: switch.bed_fan


##Weather Tab#############################################################################################
  - title: Weather
    id: 7
    icon: mdi:weather-lightning-rainy
    cards:
##Compact Header Config
      - type: custom:compact-custom-header

      - type: weather-forecast
        title: Weather
        #show_header_toggle: true
        entity: weather.dark_sky


##Settings Tab#############################################################################################
  - title: Settings
    id: 8
    icon: mdi:settings-outline
    cards:
##Compact Header Config
      - type: custom:compact-custom-header

      - type: custom:tracker-card

